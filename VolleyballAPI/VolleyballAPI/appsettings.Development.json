{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "defaultConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=RentalPageDB;Integrated Security=True;Connect Timeout=30;Encrypt=False;Trust Server Certificate=False;Application Intent=ReadWrite;Multi Subnet Failover=False"
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "default_route": {
        "ClusterId": "localhost_cluster",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Priority": 0
      },
      "api_route": {
        "Match": {
          "Path": "/api/{**catch-all}"
        },
        "Priority": 1
      }
    },
    "Clusters": {
      "localhost_cluster": {
        "Destinations": {
          "app_server": {
            "Address": "http://localhost:4200"
          }
        }
      }
    }
  }
}
